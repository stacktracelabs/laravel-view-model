{
    "name": "stacktrace/laravel-view-model",
    "description": "View Model package for Laravel.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "StackTrace\\ViewModel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "StackTrace\\ViewModel\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Peter Stovka",
            "email": "ps@stacktrace.sk"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.0|^8.1|^8.2|^8.3",
        "illuminate/contracts": "^10.0|^11.0",
        "illuminate/support": "^10.0|^11.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9",
        "nunomaduro/collision": "^7.0",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "scripts": {
        "test": "./vendor/bin/pest"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
